{
  "manifest_version": 3,
  "name": "typeFake",
  "version": "1.0.0",
  "description": "Human-typing simulator for Chrome",
  "permissions": [
    "activeTab",
    "scripting",
    "clipboardRead"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "typeFake"
  },
  "background": {
    "service_worker": "background.js"
  }
}
